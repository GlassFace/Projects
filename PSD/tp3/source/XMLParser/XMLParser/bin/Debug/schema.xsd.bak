<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

<xs:element name="module">
  <xs:complexType>
    <xs:sequence>
      <xs:element name="name" type="xs:string" minOccurs="1" maxOccurs="1" nillable="false"/>
      <xs:element name="description" type="xs:string" minOccurs="0" nillable="true" maxOccurs="1"/>
      <xs:element name="author" type="xs:string" minOccurs="0" nillable="true" maxOccurs="1"/>
      <xs:element name="creationDate" type="xs:date" minOccurs="0" nillable="true" maxOccurs="1"/>
      <xs:element name="modifiedDate" type="xs:date" minOccurs="0" nillable="true" maxOccurs="1"/>
	  <xs:element name="version" type="xs:string" minOccurs="0" nillable="true" maxOccurs="1"/>
  
      <xs:element name="CAN" minOccurs="1" maxOccurs="1" nillable="false">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="IDfields" minOccurs="1" maxOccurs="1" nillable="false">
              <xs:complexType>
                <xs:sequence>
              
			      <xs:element name="top" minOccurs="1" maxOccurs="1" nillable="false">
			        <xs:complexType>
                        <xs:simpleContent>
                          <xs:extension base="xs:string">
                            <xs:attribute name="type" type="xs:string" />
                          </xs:extension>
                        </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
			  
			      <xs:element name="middle" minOccurs="1" maxOccurs="1" nillable="false">
			        <xs:complexType>
                        <xs:simpleContent>
                          <xs:extension base="xs:string">
                            <xs:attribute name="type" type="xs:string" />
                          </xs:extension>
                        </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
			  
			      <xs:element name="low" minOccurs="1" maxOccurs="1" nillable="false">
			        <xs:complexType>
                        <xs:simpleContent>
                          <xs:extension base="xs:string">
                            <xs:attribute name="type" type="xs:string" />
                          </xs:extension>
                        </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
			  
			    </xs:sequence>
		      </xs:complexType>
            </xs:element>
            <xs:element name="format" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
  
      <xs:element name="GUI">
        <xs:complexType>
          <xs:sequence>
		    <xs:element name="method" type="xs:string"/>
		    <xs:element name="params">
              <xs:complexType>
                <xs:sequence maxOccurs="unbounded">
                  <xs:element name="param" nillable="true" minOccurs="0" maxOccurs="unbounded">
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="xs:string">
                          <xs:attribute name="name" type="xs:string" />
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
		    </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
  
    </xs:sequence>
  </xs:complexType>
</xs:element>

</xs:schema>