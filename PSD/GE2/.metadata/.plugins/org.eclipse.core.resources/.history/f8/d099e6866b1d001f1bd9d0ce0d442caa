import java.awt.Frame;

import javax.swing.JLabel;


public class StartPoint extends Frame{

	public static void main(String[] args) {
		new StartPoint();
	}
	
	StartPoint() {
		JLabel jlbHelloWorld = new JLabel("Branch Li");
		setTitle("Branch Library Sytem");
		add(jlbHelloWorld);
		this.setSize(100, 100);
		setVisible(true);


	}

}
