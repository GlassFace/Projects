package applicationLayer;

import java.util.List;

import dataStorageLayer.BookStorageFacade;

class Keeper extends Borrower{
	
	public List<Book> getBooksResponsibleFor() throws Exception{
		
		BookStorageFacade bsf = BookStorageFacade.getInstance();
		List<BranchBook> bookList = BookStorageFacade.getBooksBy("keeper", this.getId());
		
		return bookList;
	}

}
