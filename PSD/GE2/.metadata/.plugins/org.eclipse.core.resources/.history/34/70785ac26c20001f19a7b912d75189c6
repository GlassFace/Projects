package applicationLayer;

public enum Privilege {
	LIBRARIAN,
	USER,
	BORROWER,
	KEEPER,
	ADMINISTRATOR;
	
	private final int id;
	States(int i) {
		this.id = i;
	}
	
	public int getId() {
		return this.id;
	}
	
	public static States getStateById(int i) {
		for (States s : States.values()) {
			if (s.id == i)
				return s;
		}
		return States.INCENTRAL;
	}
}
