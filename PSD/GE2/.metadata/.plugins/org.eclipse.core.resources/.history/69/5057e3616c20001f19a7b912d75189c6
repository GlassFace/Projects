package applicationLayer;

public enum States {
	REQUESTRETURN (0),
	LENT (1),
	WITHKEEPER (2),
	INTRANSITTOCENTRAL (3),
	INTRANSITTOBRANCH (4),
	INCENTRAL (5);
	
	private final int id;
	States(int i) {
		this.id = i;
	}
	
	public int getId() {
		return this.id;
	}
	
	public static States getStateById(int i) {
		States s = i;
		return States;
	}
}
