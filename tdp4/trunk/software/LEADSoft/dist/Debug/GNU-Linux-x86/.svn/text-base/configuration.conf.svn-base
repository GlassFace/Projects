###############################################################################
# Screen Configuration
###############################################################################
fullscreen=1
double_buffering=1
screen_width=800
screen_height=480
screen_bpp=16
background_color=0

###############################################################################
# Level Configuration
###############################################################################
speed_source=1
max_rpm=15000
eng_temp_warn_level=110
coolant_temp_warn_level=110
oil_temp_warn_level=110
fuel_low_warn_level=20

###############################################################################
# Logger Configuration
###############################################################################
logenable=1
logappend=1
logfile=can.log

###############################################################################
# SPI Configuration
#   Note: Do not modify if you don't know what you are doing
###############################################################################
spi_device=/dev/spidev1.1
spi_mode=1
spi_bits=8
spi_speed=100000
spi_delay=0
